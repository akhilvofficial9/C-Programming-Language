#include<stdio.h>
#include<conio.h>
void main()
{
int a,b,c;
clrscr();
printf("Enter the values: a,b,c:\n");
scanf("%d%d%d",&a,&b,&c);
printf("Value of a is %d,\nb is %d,\nand c is %d",a,b,c);
getch();
}
